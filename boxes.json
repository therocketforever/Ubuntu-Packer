{
    "builders": [{
        "name": "ubuntu-1310",
        "type": "vmware",
        "guest_os_type": "Ubuntu_64",
        "iso_url": "iso/ubuntu-13.10-server-amd64.iso",
        "iso_checksum": "4d1a8b720cdd14b76ed9410c63a00d0e",
        "iso_checksum_type": "md5",
        "ssh_username": "vagrant",
        "ssh_password": "vagrant",
        "shutdown_command": "shutdown -P now",
        "ssh_wait_timeout": "10000s",
        "disk_size": 10140,
        "ssh_port": 22,
        "headless": false,
        "boot_wait": "5s",
        "boot_command": [
            "<esc><esc><enter><wait>",
            "/install/vmlinuz noapic preseed/url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ubuntu-13.10.cfg ",
            "debian-installer=en_US auto locale=en_US kbd-chooser/method=us ",
            "hostname={{ .Name }} ",
            "fb=false debconf/frontend=noninteractive ",
            "keyboard-configuration/modelcode=SKIP keyboard-configuration/layout=USA keyboard-configuration/variant=USA console-setup/ask_detect=false",
            "initrd=/install/initrd.gz -- <enter>"
        ],
        "http_directory": "/var/www"
    }],
    "provisioners": [
        {
			"type": "shell",
			"scripts": [
				"scripts/base.sh",
				"scripts/vagrant.sh",
				"scripts/cleanup.sh"
			],
			"override": {
				"ubuntu-1310": {
					"execute_command": "echo 'vagrant'|sudo -S sh '{{.Path}}'"
				}
			}
		}
    ],
    "post-processors": [
        {
            "type": "vagrant",
            "only": ["ubuntu-1310"],
            "output": "boxes/ubuntu-13.10.box",
            "vagrantfile_template": "provisions/Vagrantfile"
        }
    ]
}
